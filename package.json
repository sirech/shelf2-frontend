  "scripts": {
    "start": "vite",
    "build": "vite build",
    "pretest": "tsc --noEmit",
    "test": "vitest run",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss)\"",
    "linter:js": "npx eslint --config .eslintrc.js 'src/**/*.{ts,tsx}' --max-warnings 0",
    "linter:css": "npx stylelint 'src/**/*.css'",
    "stub": "bin/launch-stub-server",
    "bundle": "npm run build && source-map-explorer 'dist/assets/*.js'",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --config .eslintrc.js --max-warnings 0",
    "*.scss": "./go linter-css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  }
}
